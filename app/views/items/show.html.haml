    -#   .items__box
    -#     = render @myItems

    %section.items__user__profile__box
      %h2.items__user__profile__box-head
        %a.items__user__profile__box-head-title
          コーチのショルダーバッグ その他の商品
      .items__box
        .items__box__content
          %a.item__page
            .item__page-photo
              %img.picture{src: "data:image/jpeg;base64,"}/
            .item__page__body
              %h3.item__page__body-name
                coach バッグ
              .item__page__body-price
                ¥ 40,000
                .favourite
                  %i.fa.fa-heart-o.like
                  %span 28
              %p.item__page__body-tax
                (税込)

        .items__box__content
          %a.item__page
            .item__page-photo
              %img.picture{src: "data:image/jpeg;base64,"}/
            .item__page__body
              %h3.item__page__body-name
                コーチ　カバン
              .item__page__body-price
                ¥ 88,000
                .favourite
                  %i.fa.fa-heart-o.like
                  %span 39
              %p.item__page__body-tax
                (税込)

        .items__box__content
          %a.item__page
            .item__page-photo
              %img.picture{src: "data:image/jpeg;base64,"}/
            .item__page__body
              %h3.item__page__body-name
                coach bag
              .item__page__body-price
                ¥ 125,999
                .favourite
                  %i.fa.fa-heart-o.like
                  %span 12
              %p.item__page__body-tax
                (税込)

        .items__box__content
          %a.item__page
            .item__page-photo
              %img.picture{src: "data:image/jpeg;base64,"}/
            .item__page__body
              %h3.item__page__body-name
                Coach 限定品
              .item__page__body-price
                ¥ 189,999
                .favourite
                  %i.fa.fa-heart-o.like
                  %span 5
              %p.item__page__body-tax
                (税込)

        .items__box__content
          %a.item__page
            .item__page-photo
              %img.picture{src: "https://encrypted-tbn0.gstatic.com/images?q=tbn:ANd9GcR8chqob3X9cXAK5E3r8sso0KdR7i1kG7qmuenFmW571huyLWqj"}/
            .item__page__body
              %h3.item__page__body-name
                コーチバッグ
              .item__page__body-price
                ¥ 90,000
                .favourite
                  %i.fa.fa-heart-o.like
                  %span 64
              %p.item__page__body-tax
                (税込)

        .items__box__content
          %a.item__page
            .item__page-photo
              %img.picture{src: "https://encrypted-tbn0.gstatic.com/images?q=tbn:ANd9GcTMP2TdmsE6bxt5NNMIyv0TgrQ8R6phfsh4fR5sQxYLCAJq2L0J"}/
            .item__page__body
              %h3.item__page__body-name
                Coach Bag
              .item__page__body-price
                ¥ 135,000
                .favourite
                  %i.fa.fa-heart-o.like
                  %span 96
              %p.item__page__body-tax
                (税込)

  =render "layouts/footer"

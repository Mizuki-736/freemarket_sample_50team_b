.solo-countainer
  .solo-header
    .logo
      = link_to image_tag("https://www-mercari-jp.akamaized.net/assets/img/common/common/logo.svg", alt: "mercari"), :back, class: 'session-home'
  .solo-main
    .container
      .main-head
        ="新規会員登録"
      .main-inner
        .main-content
          .btn-default
            %button.btn-default.btn-mail
              %i.fa.fa-envelope-o
              ="メールアドレスで登録"
          .btn-default
            .btn-sns
              %button.btn-default.btn-sns.btn-sns-facebook
                %i.fa.fa-facebook-f
                ="Facebookで登録"
          .btn-default
            .btn-sns
              %button.btn-default.btn-sns.btn-sns-google
                ="Googleで登録"
  =render "layouts/session-footer"
